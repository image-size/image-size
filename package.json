  "description": "get dimensions of any image file",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "bin/image-size.js"
  ],
  "engines": {
    "node": ">=10.18.0"
  },
  "bin": {
