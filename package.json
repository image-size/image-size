    "@types/sinon": "9.0.11",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "chai": "4.3.4",
    "eslint": "7.22.0",
    "glob": "7.1.6",
    "mocha": "8.3.2",
    "nyc": "15.1.0",
    "sinon": "9.2.4",
    "ts-node": "9.1.1",
    "typedoc": "0.20.32",
